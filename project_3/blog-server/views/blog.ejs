<!DOCTYPE html>
<html>

<head>
  <title><%= username %>'s Blog</title>
  <link rel="stylesheet" href="/stylesheets/style.css" />
</head>

<body>
  <h1><%= username %>'s blog</h1>
  <ul>
    <% posts.forEach((post) => { %>
    <li><%- post.title %></li>
    <ul>
      <li><%- post.body %></li>
      <li><%= post.created %></li>
      <li><%= post.modified %></li>
    </ul>
    <% }); %>
  </ul>
  <% if (nextPageUrl) { %>
  <a id="next" href=<%= nextPageUrl %>>Next</a>
  <%}; %>
</body>


</html>